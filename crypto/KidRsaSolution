from Crypto.Util.number import *
a = 9771982271483414071
b = 15708970666624921667
a1 = 13818138669123491509
b1 = 10933781350997079767

M =  a * b -1

e =  a1 * M + a

d =  b1 * M + b

n = (e * d )/M 

c = 17368117908321098491686000607393931367250036402887538274330077733586892297732

m = (c*d)%n

print "m in plaintext:  ",long_to_bytes(m)
